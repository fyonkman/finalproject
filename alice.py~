from pyp2p.net import *
import time

alice = Net(passive_bind="deoni.cs.williams.edu", passive_port=9173, interface="etho:2", node_type="passive", debug=1)
alice.start()
print("started")
alice.bootstrap()
alice.advertise()

while 1:
    print("in while")
    for con in alice:
        print("hello")
        for reply in con:
            print(reply)

    time.sleep(1)

